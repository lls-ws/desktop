[begin] (Menu LLS Tecnologia)
[encoding] {utf8}
	
    [exec] (Chrome) 						{google-chrome} 					</opt/google/chrome/product_logo_32.xpm>
    [exec] (Geany) 	 						{geany} 							</usr/share/pixmaps/geany.xpm>
    [exec] (Audacious) 						{audacious -p} 						</usr/share/pixmaps/audacious-32.xpm>
    [exec] (Terminal) 						{gnome-terminal --hide-menubar} 	<>
   
[separator]

	[exec] (Gerenciador de Arquivos)  		{nautilus}   						</usr/share/pixmaps/nautilus.xpm>
	[exec] (StreamTuner) 					{streamtuner2} 						</usr/share/pixmaps/streamtuner2.xpm>
	[exec] (Opera) 							{opera} 							</usr/share/pixmaps/opera.xpm>

[separator]

[submenu] (Multimedia) <~/.fluxbox/pixmaps/multimedia.xpm>
      	[exec] (Parole) 					{parole} 							<~/.fluxbox/pixmaps/parole.xpm>
      	[exec] (Volume) 					{pavucontrol} 						<~/.fluxbox/pixmaps/multimedia.xpm>
[end]

[submenu] (Office) <~/.fluxbox/pixmaps/office.xpm>
      	[exec] (Gerenciador de Arquivos)  	{thunar}   							<~/.fluxbox/pixmaps/thunar.xpm>
      	[exec] (Visualizador de Imagens)  	{ristretto}   						<~/.fluxbox/pixmaps/ristretto.xpm>      	
      	[exec] (Editor de Texto) 			{mousepad} 							<~/.fluxbox/pixmaps/mousepad.xpm>
      	[exec] (Calculadora) 				{mate-calc} 						<~/.fluxbox/pixmaps/mate-calc.xpm>
[end]

[submenu] (Developer) <~/.fluxbox/pixmaps/developer.xpm>
		[exec] (ireport) 	 				{ireport} 							<~/.fluxbox/pixmaps/java.xpm>
[end]

[submenu] (Tools) <~/.fluxbox/pixmaps/tools.xpm>
      	[exec] (Desligar o monitor) 	  	{xset -display :0.0 dpms force off}			<~/.fluxbox/pixmaps/shutdown.xpm>
      	[exec] (Finalizar processo) 	  	{xkill} 									<~/.fluxbox/pixmaps/xkill.xpm>
[end]

[submenu] (Menu Fluxbox) <~/.fluxbox/pixmaps/anonymous.xpm>
      [config] (Configuracao) 
[submenu] (Estilos do Sistema) {Escolha um estilo...}
      [stylesdir] (/usr/share/fluxbox/styles) 
[end]
[submenu] (Estilos do Utilizador) {Escolha um estilo...}
      [stylesdir] (~/.fluxbox/styles)
[end]
      [workspaces] (Lista de Áreas de Trabalho) 
[submenu] (Ferramentas)
      [exec] (Nome da Janela) 	{xprop WM_CLASS|cut -d \" -f 2|xmessage -file - -center} 
      [exec] (Capturar Tela) 	{/usr/bin/captura_tela}
[end]
      	[exec] (Configuracoes de Inicio) 		{geany ~/.fluxbox/startup} 
      	[exec] (Configuracoes do Menu) 			{geany ~/.fluxbox/menu}
      	[exec] (Configuracoes de Apps) 			{geany ~/.fluxbox/apps}
      	[exec] (Configuracoes de Teclas) 		{geany ~/.fluxbox/keys}
      	[exec] (Configuracoes do GTK-2.0) 		{geany ~/.gtkrc-2.0}
      	[exec] (Configuracoes do GTK-3.0) 		{geany ~/.config/gtk-3.0/settings.ini}
      	[exec] (Executar) 						{fbrun} 
      	[reconfig] (Recarregar configuracao) 
      	[restart]  (Reiniciar) 
      	[exec]     (Sobre) {(fluxbox -v; fluxbox -info | sed 1d) 2> /dev/null | xmessage -file - -center} 
      	[separator] 
      	[exit] (Sair) 									<~/.fluxbox/pixmaps/logout.xpm>
[end]

      [separator] 
      [exec] (Reiniciar) 		{sudo /sbin/reboot} 								<~/.fluxbox/pixmaps/reiniciar.xpm>
      [exec] (Desligar)  		{sudo /sbin/shutdown -h now} 						<~/.fluxbox/pixmaps/desligar.xpm>
[endencoding]
[end]
